{
  "name": "angularjs.xlts.dev",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "start": "ng serve --port=4201",
    "build": "ng build",
    "build:sm": "ng build --configuration=staging --source-map",
    "build:prod": "ng build --prod && npm run scully",
    "test": "ng test",
    "lint": "ng lint",
    "lint:styles": "stylelint '**/*.scss' --config .stylelintrc --syntax scss",
    "e2e": "ng e2e",
    "scully": "scully --nw",
    "scully:watch": "scully",
    "scully:serve": "scully serve",
    "smex": "source-map-explorer",
    "deploy": "firebase use default && firebase deploy",
    "update": "npm-check -u"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@schuchard/prettier": "^3.1.0",
    "@scullyio/init": "~0.0.23",
    "@scullyio/ng-lib": "~0.0.22",
    "@scullyio/scully": "~0.0.83",
    "firebase": "~7.13.1",
    "logrocket": "^1.0.7",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "<0.900 || ^0.900.0-0 || ^9.0.0-0",
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "firebase-tools": "^7.12.0",
    "fuzzy": "^0.1.3",
    "husky": "~4.2.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "10.0.9",
    "npm-check": "^5.9.2",
    "prettier": "~2.0.2",
    "protractor": "~5.4.3",
    "rxjs-tslint-rules": "^4.29.0",
    "source-map-explorer": "^2.3.1",
    "stylelint": "^13.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "~1.18.0",
    "typescript": "~3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}
