{
  "projects": {
    "default": "xlts-dev",
    "production": "xlts-dev"
  },
  "hosting": [
    {
      "target": "angularjs.xlts.dev",
      "public": "dist/static",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
